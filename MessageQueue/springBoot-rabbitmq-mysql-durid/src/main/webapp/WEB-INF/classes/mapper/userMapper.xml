<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.songjiuhua.bootmain.mvc.model.TrainNews">
    <!--根据id查询用户详情-->
    <select id="queryById" parameterType="int" resultType="com.songjiuhua.bootmain.mvc.model.TrainNews">
        SELECT id, news_type as newsType, city_name as cityName FROM train_news WHERE id=#{id}
    </select>
</mapper>